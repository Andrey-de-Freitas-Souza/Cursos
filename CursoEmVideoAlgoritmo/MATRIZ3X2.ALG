Algoritmo "Matriz3x2"

Var
 m: vetor[1..3, 1..2]  de inteiro
 l,c: inteiro

Inicio
  para l <- 1 ate 3 faca
    para C <- 1 ate 2 faca
         Escreva("Digite o valor da posicao [" , l , ","  ,c , "]: ")
         leia(m[l,c])
    FimPara
  FimPAra
  para l <- 1 ate 3 faca
    para C <- 1 ate 2 faca
         Escreva(m[l,c])
    FimPara
    EscrevaL()
  FimPAra

Fimalgoritmo